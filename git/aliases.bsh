alias gs='git status'
alias ga='git add'
alias gc='git commit -m'
alias gp='git push origin'
alias gpm='git push origin master'
alias gph='git push origin gh-pages'
alias gu='git pull origin'
alias gum='git pull origin master'
alias guh='git pull origin gh-pages'
alias ghpages='git symbolic-ref HEAD refs/heads/gh-pages && rm .git/index && git clean -fdx'
